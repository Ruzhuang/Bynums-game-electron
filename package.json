{
    "name": "Bynums-game-electron",
    "version": "1.0.0",
    "description": "This is a cross platform shoppinglist desktop application built on electron",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns",
        "installer-mac": "electron-installer-dmg Bynums-game-electron-darwin-x64/Bynums-game-electron.app Bynum-installer",
        "package-win": "electron-packager . --platform=win32 --icon=assets/icons/win/icon.ico",
        "package-linux": "electron-packager . --platform=linux --icon=assets/icons/png/icon.png",
        "zip-linux": "electron-installer-zip Bynums-game-electron-linux-x64/Bynums-game-electron linux-zip"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "electron": "^12.0.0"
    },
    "devDependencies": {
        "electron": "^12.0.0",
        "electron-installer-dmg": "^3.0.0",
        "electron-packager": "^15.2.0"
    }
}